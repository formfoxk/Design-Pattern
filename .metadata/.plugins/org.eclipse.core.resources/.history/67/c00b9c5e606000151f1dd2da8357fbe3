package account;

import classexample.Duck;
import classexample.FlyRocketPowered;
import classexample.RubberDuck;

public class AccountSimulation {
	public static void Main(String[] args){
		Account mallard = new NormalAccount();
		mallard.performFly();

		Duck rubber = new RubberDuck();
		rubber.performFly();

		rubber.setFlyBehavior(new FlyRocketPowered()); // 실행 중 행위 변경
		rubber.performFly();
	}
}
