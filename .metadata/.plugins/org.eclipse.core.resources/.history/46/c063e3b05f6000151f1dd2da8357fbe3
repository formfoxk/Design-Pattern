package account;

public class SavingAccount extends Account{
	
	
	public SavingAccount(double limitedMoney){
		super();
		this.limitedMoney = limitedMoney;
	}
	public SavingAccount(double balance, double limitedMoney){
		super(balance);
		this.limitedMoney = limitedMoney;
	}	
	
	public String toString(){
		return "--- MinusAccount ---\nCreateDate : " + createDate + "\nBalance : " + balance;
	}
}
