package account;

public class MinusWithdrawTransaction extends WithdrawTransaction{

	private double limitedMoney;
	
	public MinusWithdrawTransaction(double limitedMoney) {
		// TODO Auto-generated constructor stub
		this.limitedMoney = limitedMoney;
	}
	
	public double execute(double money){
		double result = (balance+limitedMoney) - money;
		if(result > 0)
			return result;
		return -1;
	}
}
