package classexample;

public class RedheadDuck extends Duck {
	// 각 오리 객체 생성 時 (생성자에서) 자신의 행동 설정
	public RedheadDuck() { flyBehavior = new FlyWithWings(); }

	public void quack() { System.out.println("quack of MallardDuck"); }
	public void display() { System.out.println("I'm a real Red head duck"); }
}